package collections;

public class Student {
	private String student;
	private int id;

	public Student(String student, int id) {

		this.student = student;
		this.id = id;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getStudent() {
		return student;
	}

	public void setStudent(String student) {
		this.student = student;
	}

}
