
public class BiNumber {

	private int number1;
	private int number2;

	public BiNumber(int number1, int number2) {
		// TODO Auto-generated constructor stub
		this.number1 = number1;
		this.number2 = number1;
	}

	public int add() {
		return this.number1 + this.number2;
	}

	public int multiply() {
		// TODO Auto-generated method stub
		return this.number1 * this.number2;
	}

	public int doublen() {
		// TODO Auto-generated method stub
		this.number1 *= 2;
		this.number2 *= 2;

	}


}
