
public class GamerRunner {

	public static void main(String[] args) {

		GamingConsole game = new MarioGame();

		GamingConsole chessgame = new ChessGame();
		game.up();
		game.down();
		game.left();
		game.right();

		chessgame.down();
		chessgame.up();
		chessgame.left();
		chessgame.right();
	}

}
