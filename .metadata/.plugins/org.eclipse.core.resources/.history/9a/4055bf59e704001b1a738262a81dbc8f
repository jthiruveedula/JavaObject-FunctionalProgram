
public class IfStatementRunner {

	public static void main(String[] args) {
		int i = 45;
		if (i == 45) {
			System.out.println("i = 45");
		}
		else if(i==24) {
			System.out.println("i = 24");
		}
		else if(i==30) {
			System.out.println("i = 30");
		}
		else {
			System.out.println("i is nothing");
		}

	}

}
